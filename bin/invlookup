#!/bin/bash

IPN=$(cat ~/install/inv.txt | rofi -x11 -normal-window -dmenu -i -matching-negate-char '\0' | sed 's/ .*//')

[[ ! -z "$IPN" ]] && firefox http://collabserver.org:10003/part/$IPN/

if [ ! -f ~/install/inv.txt ] || test "`find ~/install/inv.txt -mmin +10`"
then
    ~/dotfiles/bin/invrefresh
fi
